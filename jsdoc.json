{
    "plugins": ["jsdoc-http-plugin"],
    "source" : {
        "include": ["server","client"],
        "includePattern" : ".js$",
        "excludePattern" : "(node_modules|docs)"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks" : false
    },
    "opts" : {
        "recurse" : "true",
        "destination": "./doc",
        "template": "node_modules/docdash",
        "tutorials": "./tutorials",
        "readme": "./README.md"
       
    }
}